                <TextView
                    android:id="@+id/Tenant_occ_edt"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/shadow"
                    android:layout_below="@id/Tenant_occ"
