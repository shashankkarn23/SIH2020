                <TextView
                    android:id="@+id/Tenant_office_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_below="@id/Tenant_occ_edt"
