                    android:text="Name"/>

                <TextView
                    android:id="@+id/Tenant_name_edt"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_toLeftOf="@id/imageView_te"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/shadow"
                    android:layout_below="@id/Tenant_name"
                    android:paddingHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp"/>

                <TextView
                    android:id="@+id/Tenant_age"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_below="@id/Tenant_name_edt"
                    android:text="Age"/>

                <TextView
                    android:id="@+id/Tenant_age_edt"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:background="@drawable/shadow"
                    android:paddingHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:layout_below="@id/Tenant_age"
                    android:textColor="#000000"
                    android:layout_marginRight="20dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp"/>

                <TextView
                    android:id="@+id/Tenant_mob"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_below="@id/Tenant_name_edt"
                    android:layout_toRightOf="@+id/Tenant_age_edt"
                    android:text="Mobile"/>

                <TextView
                    android:id="@+id/Tenant_mob_edt"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/shadow"
                    android:paddingHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:layout_below="@id/Tenant_mob"
                    android:layout_toRightOf="@id/Tenant_age_edt"
                    android:textColor="#000000"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"/>

                <TextView
                    android:id="@+id/Tenant_F_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
